from time import time
from time import sleep
st = time()

a = 1213121123589312131213121312131
b = 12112565448945498716877944
c = 325777777
s = a
def recurring(s):
    ans = []
    #s = str(1/n).split(".")[1]
    if len(s) < 16:
        return ["1"]
    for i in range(len(s)):
        ind = i
        while True:
            try :
                ind = s.index(s[i], ind+1)
                if s[i:ind] == s[ind:2*ind-i] and s[i:ind*2-i] == s[i:] and len([s[i:ind],s[ind:2*ind-i]])!=len(s):
                    ans += [s[i:ind],s[ind:2*ind-i]]
            except:
                break
    return ans if ans!=[] else ["1"]

def su(s):
    t =0
    for i in s:
        t+=int(i)
    return t
ans = []
ma = 0
ri = ""
#print(len(str(s)))
for i in range(1,1000):
    ri = ""+(str())
    
    r = recurring(ri)
    ans += [min(map(len, r))]
    #print(i,ri,"\n",min(map(len, r)),min(map(int, r)))

#print(ans)
print(len(ans))
print('max', max(ans))
print("len", ans.index(max(ans)))
print('________THE___________%s__________END________'%(time() - st))
